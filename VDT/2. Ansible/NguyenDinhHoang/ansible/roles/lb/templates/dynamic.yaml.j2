http:
  routers:
    vdt-frontend:
      rule: Host(`"{{ FRONTEND_DOMAIN }}"`)
      entryPoints:
        - web
      service: vdt-frontend
      tls:
        certResolver: production
  services:
    vdt-frontend:
      loadBalancer:
        servers:
          - url: http://"{{ VM1_IP }}":3000
          - url: http://"{{ VM2_IP }}":3000
          - url: http://"{{ VM3_IP }}":3000
